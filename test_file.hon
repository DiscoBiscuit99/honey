# another function declaration
let f: (x: number) -> number = {
  let b: number = x * 2;
  let c: number = b * 3;
  c
};

# function call
let h: number = f(3 + 4);

# fibonacci
let fib: (n: number) -> number = {
  if n < 2 {
    1
  } else {
    fib(n - 1) + fib(n - 2)
  }
};

# conditionals (in functions)
